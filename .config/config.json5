{
  "keybindings": {
    "Normal": {
      "<Ctrl-d>": "Quit", // Another way to quit
      "<Ctrl-c>": "Quit", // Yet another way to quit
      "<Ctrl-z>": "Suspend", // Suspend the application
      "<Ctrl-r>": "Refresh",
      "<i>": "InputMode",
      "<Tab>": "Tab",
      "<Backtab>": "BackTab",
      "<Ctrl-o>": "ProcessReplace",
    },
    "Input": {
      "<Esc>": "NormalMode",
      "<j><k>": "NormalMode"
    }
  }
}
